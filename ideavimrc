source ~/.config/nvim/init.vim

nnoremap <C-k> :action CodeCompletion<CR>

nnoremap <leader>b :action Build<CR>
nnoremap <leader>r :action Run<CR>
nnoremap <leader>dr :action Debug<CR>
nnoremap <leader>db :action ToogleLineBreakpoint<CR>

nnoremap <leader>of :action GotoClass<CR>
nnoremap <leader>oa :action GotoFile<CR>
nnoremap <leader>gd :action GotoDeclaration<CR>
nnoremap <leader>tt :action GotoTest<CR>
nnoremap <leader>en :action GotoNextError<CR>
nnoremap <leader>ep :action GotoPreviousError<CR>

nnoremap <leader><leader> :action HideAllWindows<CR>
nnoremap <leader><leader>d :action ToggleDistractionFreeMode<CR>


